// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Shipment {
  id       String @id
  state    String
  contents Item[]
}

model Item {
  id         String   @id
  shipmentId String
  shipment   Shipment @relation(fields: [shipmentId], references: [id])
}
